
{
    "manifest_version":2,
    "name":"repeatApp",
    "version":"1.0.0",
    "description":"repeat",
    "icons":
    {
        "16":"logo.png",
        "48":"logo.png",
        "128":"logo.png"
    },
    "content_scripts":
    [
        {
            "matches":
            [
                "<all_urls>"
            ],
            "css":
            [
                "app.css"
            ],
            "js":
            [
                "manifest.js",
                "vendor.js",
                "app.js"
            ],
            "run_at":"document_end"
        }
    ],
    "permissions":
    [
        "webRequest",
        "webRequestBlocking",
        "http://*/*",
        "https://*/*"
    ]
}

